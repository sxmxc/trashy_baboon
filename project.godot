[application]

config/name="trashy-baboon"
config/description="Enter an interesting project description here!"
run/main_scene="res://Scenes/StartScreen/StartScreen.tscn"
config/icon="res://icon.png"
config/version="0.1.0a"

[autoload]

SaveManager="*res://Core/SaveManager.gd"
Global="*res://Core/Global.tscn"
SceneManager="*res://addons/scene_manager/SceneManager.tscn"

[display]

window/size/width=640
window/size/height=360
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor]

version_control/autoload_on_startup=true
version_control/plugin_name="GitAPI"

[editor_plugins]

enabled=PoolStringArray( "res://addons/AutoExportVersion/plugin.cfg", "res://addons/dialogic/plugin.cfg", "res://addons/scene_manager/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

